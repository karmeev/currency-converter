{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiter": {
    "PermitLimit": 1,
    "DurationMilliseconds": 1,
    "QueueLimit": 1,
    "RejectionStatusCode": 429,
    "QueueOrder": "OldestFirst"
  },
  "Infrastructure": {
    "Jwt": {
      "Issuer": "",
      "Audience": "",
      "AccessTokenTtlInMinutes": 2,
      "RefreshTokenTtlInDays": 360,
      "SecurityKey": ""
    },
    "Redis": {
      "ConnectionString": "",
      "EntitiesDatabaseNumber": 0,
      "RefreshTokensDatabaseNumber": 1
    },
    "Integrations": {
      "Frankfurter": {
        "BaseAddress": "",
        "TimeoutSeconds": 0,
        "RetryCount": 1,
        "RetryExponentialIntervalSeconds": 5,
        "CircuitBreakerDurationBreakSeconds": 30,
        "CircuitBreakerMaxExceptions": 2
      }
    }
  },
  "Users": []
}
